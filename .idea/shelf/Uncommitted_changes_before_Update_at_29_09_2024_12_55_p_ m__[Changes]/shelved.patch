r: var(--text-color);\r\n    padding: 10px;\r\n}\r\n\r\n.btn_primary{\r\n    background-color: #00b700;\r\n    color: white;\r\n}\r\n\r\nbutton:hover{\r\n    background-color: var(--footer-bg-color);\r\n}\r\n\r\n\r\nfooter {\r\n    background-color: var(--secondary-color);\r\n    color: var(--primary-color);\r\n    text-align: center;\r\n    padding: 1px 0;\r\n    position:fixed;\r\n    bottom: 0;\r\n    width: 100%;\r\n}\r\nhtml, body {\r\n    height: 100%;\r\n}\r\n.full-height {\r\n    min-height: 70vh;\r\n    min-width: 70vh;/* Asegura que el contenedor tenga al menos la altura de la ventana */\r\n    display: flex;\r\n    justify-content: center; /* Centra horizontalmente */\r\n    align-items: center; /* Centra verticalmente */\r\n}\r\n.full-height_l {\r\n    min-height: 50vh;\r\n    min-width: 50vh;/* Asegura que el contenedor tenga al menos la altura de la ventana */\r\n    display: flex;\r\n    justify-content: center; /* Centra horizontalmente */\r\n    align-items: center; /* Centra verticalmente */\r\n}\r\n.content {\r\n    text-align: center; /* Alinea el texto en el centro */\r\n}
